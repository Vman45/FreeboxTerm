{
  "name": "FreeboxTerm",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node .",
    "dev": "NODE_ENV=dev node .",
    "lint": "eslint src",
    "build": "babel src --out-dir build && PYTHON=/usr/bin/python2 ./node_modules/.bin/nexe"
  },
  "nexe": {
    "input": "build/index.js",
    "output": "freebox",
    "python": "/usr/bin/python2",
    "browserify": {
      "paths": [
        "./build",
        "./node_modules"
      ]
    },
    "runtime": {
      "framework": "node",
      "version": "5.10.0"
    }
  },
  "bin": {
    "freebox": "bin.js"
  },
  "author": "Balthazar Gronon",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.20.0",
    "blessed": "^0.1.81",
    "blessed-contrib": "^3.5.5",
    "dateformat": "^1.0.12",
    "immutable": "^3.7.6",
    "q": "^1.4.1",
    "redux": "^3.3.1",
    "redux-actions": "^0.9.1",
    "redux-batched-actions": "^0.1.2",
    "redux-thunk": "^2.0.1",
    "request": "^2.71.0",
    "request-progress": "^2.0.1"
  },
  "devDependencies": {
    "app-module-path": "^1.0.6",
    "babel-cli": "^6.7.5",
    "babel-eslint": "^6.0.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.7.2",
    "eslint": "^2.7.0",
    "eslint-config-zavatta": "^3.0.1",
    "nexe": "^1.1.2",
    "piping": "^0.3.0",
    "pty.js": "^0.3.0",
    "term.js": "0.0.7"
  }
}
